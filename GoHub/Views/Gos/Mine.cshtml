@model IEnumerable<GoHub.Models.Go>
@{
    ViewBag.Title = "Mine";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>My Upcoming Gos</h2>

<ul class="gos voffset4">
    @foreach (var go in Model)
    {
        <li>
            <div class="date">
                <div class="month">
                    @go.DateTime.ToString("MMM")
                </div>
                <div class="day">
                    @go.DateTime.ToString("d ")
                </div>
            </div>
            <div class="details">

                <span class="artical">
                    @go.Venue
                    
                   
                </span>
                <span class="genre">

                    @go.Genre.Name
                </span>

               

            </div>

        </li>
    }
</ul>
